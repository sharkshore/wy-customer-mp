

/** 轮播图样式 */
.swiper {
  height: 400rpx;
  width: 100%;
}
.swiper image {
  height: 100%;
  width: 100%;
}

/** 搜索框 */
.search_row{
  background-color:#e7e7e7; 
   
  padding:10rpx 0;
}
.search_input{
  border:1px solid #b7b7b7;
  background-color:white; 
  margin:10rpx 50rpx;
  padding: 5rpx 0;
  border-radius: 5px;
  display: flex; justify-content: center; align-items: center;
}
.search_txt{
  font-family: 华文细黑;
  font-size:14px;
  color:#ababab;
}
.search_icon{
  vertical-align: -2px;
}
